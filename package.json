{
  "name": "hospbot-messenger-desktop-ce",
  "version": "1.0.0",
  "description": "A Hospbot Messenger client for the desktop - Community Edition",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=public/img/icon.icns --prune=true --out=builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=public/img/icon.ico --prune=true --out=builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"eHOSP Services\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/logo.png --prune=true --out=builds"
  },
  "repository": "https://github.com/ehosp/Hospbot-Messenger-Desktop-CE.git",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Alex Karadimos",
  "license": "",
  "devDependencies": {
    "electron": "~1.7.8",
    "electron-packager": "^9.1.0",
    "install": "^0.10.1",
    "npm": "^5.4.2"
  },
  "dependencies": {
    "botui": "^0.3.4",
    "vue": "^2.4.4"
  }
}
